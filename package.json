{
  "name": "atelier-amelie-workspace",
  "version": "0.0.0",
  "license": "MIT",
  "prisma": {
    "schema": "libs/prisma/schema.prisma",
    "seed": "ts-node --project libs/prisma/tsconfig.json libs/prisma/src/seeds/run.ts"
  },
  "scripts": {
    "docker:up": "docker-compose --env-file ./.env -f docker/atelier-amelie-workspace/docker-compose.yml up -d",
    "docker:down": "docker-compose --env-file ./.env -f docker/atelier-amelie-workspace/docker-compose.yml down",
    "dev:all": "pnpm docker-up && nx run-many --target=serve --projects=api,admin",
    "db:push": "nx run prisma:exec db push",
    "db:drop": "nx run prisma:exec migrate reset --force --skip-seed",
    "db:setup": "pnpm db:drop && pnpm db:push",
    "db:seed": "pnpm db:drop && pnpm db:push && nx run prisma:exec db seed"
  },
  "private": true,
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
    "@faker-js/faker": "^7.6.0",
    "@nrwl/cypress": "15.4.1",
    "@nrwl/eslint-plugin-nx": "15.4.1",
    "@nrwl/jest": "15.4.1",
    "@nrwl/linter": "15.4.1",
    "@nrwl/react": "15.4.1",
    "@nrwl/vite": "15.4.1",
    "@nrwl/web": "15.4.1",
    "@nrwl/workspace": "15.4.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@prisma/client": "^4.8.0",
    "@svgr/webpack": "^6.1.2",
    "@swc/core": "^1.2.173",
    "@testing-library/react": "13.4.0",
    "@types/jest": "28.1.1",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/coverage-c8": "~0.25.8",
    "@vitest/ui": "^0.25.8",
    "babel-jest": "28.1.1",
    "cypress": "^11.0.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "jsdom": "~20.0.3",
    "nx": "15.4.1",
    "prettier": "^2.6.2",
    "prisma": "^4.8.0",
    "react-refresh": "^0.10.0",
    "react-test-renderer": "18.2.0",
    "swc-loader": "0.1.15",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.8.2",
    "url-loader": "^4.1.1",
    "vite": "^4.0.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-tsconfig-paths": "^4.0.2",
    "vitest": "^0.25.8"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "regenerator-runtime": "0.13.7",
    "slugify": "^1.6.5",
    "tslib": "^2.3.0"
  }
}
